<div class="container">
  <div class="row">
    <div id="heading" class="col-xs-4">
      <h3><b>Permissions</b></h3>
    </div>
  </div>

  <div class="row">
    <hr>
  </div>

  <div class="row">
    <table>
      <tbody>
        <% @permissions.each do |perm| %>
          <tr>
            <td><%= perm.user.to_s + " (" + perm.role.to_s + ") - " + perm.room.to_s %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>