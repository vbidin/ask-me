<div class="container">
  <div class="row">
    <div id="heading" class="col-xs-4">
      <h3><b>Types</b></h3>
    </div>
  </div>

  <div class="row">
    <hr>
  </div>

  <div class="row">
    <table>
      <tbody>
        <% @types.each do |type| %>
          <tr>
            <td><%= type.id.to_s + " - " + type.name %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>